---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Oscar's Blog</title>
	<link href="http://ahhsca.github.io/webby/blog/atom.xml" rel="self"/>
	<link href="http://ahhhsca.github.io/webby/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://ahhsca.github.io/webby/blog</id>
	<author>
		<name>Oscar</name>
		<email>cheung.oscar@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://ahhsca.github.io/webby{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://ahhsca.github.io/webby{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
